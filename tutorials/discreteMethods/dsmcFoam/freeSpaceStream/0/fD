FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      fD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }

    inlet
    {
        type            zeroGradient;
    }

    sides
    {
        type            zeroGradient;
    }
}

