(
    periodicX
    {
        type cyclic;
        faces
        (
            (1 2 6 5)
            (0 4 7 3)
        );
    }

    periodicY
    {
        type cyclic;
        faces
        (
            (2 3 7 6)
            (0 1 5 4)
        );
    }

    periodicZ
    {
        type cyclic;
        faces
        (
            (0 3 2 1)
            (4 5 6 7)
        );
    }
)

mergePatchPairs
(
