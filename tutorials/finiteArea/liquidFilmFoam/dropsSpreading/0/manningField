FoamFile
{
    version     2.0;
    format      ascii;
    class       areaScalarField;
    location    "0";
    object      manningField;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 1 0 0 0 0 0];

internalField       uniform 0.01;

boundaryField
{
    left
    {
        type zeroGradient;
    }

    right
    {
        type zeroGradient;
    }

    front
    {
        type zeroGradient;
    }

    back
    {
        type zeroGradient;
    }
}

