solvers
{
    p
    {
        solver           GAMG;
        tolerance        1e-8;
        relTol           0;

        smoother         DIC; //GaussSeidel;
        nPreSweeps       0;
        nPostSweeps      2;
        nFinestSweeps    2;

        cacheAgglomeration true;
        nCellsInCoarsestLevel 10;
        agglomerator     faceAreaPair;
        mergeLevels      1;
    }
    Ua
    {
        solver           PBiCG;
