FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    object      Ub;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0.25 0);

referenceLevel      (0 0 0);

boundaryField
{
    walls
    {
        type                fixedValue;
        value               uniform (0 0 0);
    }
    outlet
    {
       type                 zeroGradient;
    }
    inlet
    {
        type                fixedValue;
        value               uniform (0 0.25 0);
    }
    frontAndBackPlanes
    {
        type                empty;
    }
}

