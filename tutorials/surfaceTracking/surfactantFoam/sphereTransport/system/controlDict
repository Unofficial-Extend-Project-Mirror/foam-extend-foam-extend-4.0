FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application    surfactantFoam;

startFrom           startTime;

startTime           0;

stopAt              endTime;

endTime             6;

deltaT              0.1;

writeControl        runTime;

writeInterval       0.2;

purgeWrite          0;

writeFormat         ascii;

writeCompression    uncompressed;

timeFormat          general;

timePrecision       6;

runTimeModifiable   yes;

// ************************************************************************* //


