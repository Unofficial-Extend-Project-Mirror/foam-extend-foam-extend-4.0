FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/faMesh";
    object      faMeshDefinition;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

polyMeshPatches  1( freeSurface );

boundary
{
    left
    {
        type                patch;
        ownerPolyPatch      freeSurface;
        neighbourPolyPatch  leftWall;
    }

    right
    {
        type                patch;
        ownerPolyPatch      freeSurface;
        neighbourPolyPatch  rightWall;
    }

    frontAndBack
    {
        type                empty;
        ownerPolyPatch      freeSurface;
        neighbourPolyPatch  frontAndBack;
    }
}

