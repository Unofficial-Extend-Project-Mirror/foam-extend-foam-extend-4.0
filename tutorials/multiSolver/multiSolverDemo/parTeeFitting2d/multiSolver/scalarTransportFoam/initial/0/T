FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "-1";
    object      scalarTransportFoam@-1@0@T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    steetWalls
    {
        type            zeroGradient;
    }
    branchWalls
    {
        type            zeroGradient;
    }
    westStreet
    {
        type            zeroGradient;
    }
    eastStreet
    {
        type            zeroGradient;
    }
    northBranch
    {
        type            fixedValue;
        value           uniform 1;
    }
    defaultFaces
    {
        type            empty;
    }
}

