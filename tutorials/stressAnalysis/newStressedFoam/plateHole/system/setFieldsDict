FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue materials 0
    volVectorFieldValue U (0 0 0)
);

regions
(
    boxToCell
    {
        box (0 0 -1) (1 0.8 1);

        fieldValues
        (
            volScalarFieldValue materials 1
        );
    }

    boxToCell
    {
        box (0 1.5 -1) (2 2 1);

        fieldValues
        (
            volScalarFieldValue materials 2
        );
    }
);

