FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     icoDyMFoam;

startFrom       startTime;

startTime       0.0;

stopAt          endTime;

endTime         4.0;

deltaT          0.01;

writeControl    timeStep;

writeInterval   5;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable yes;

adjustTimeStep yes;

maxCo 0.5;

maxDeltaT 0.25;

functions
(
    RBFMotion
    {
        type RBFMotion;
        functionObjectLibs ("libRBFMotionFunction.so");
        rotationAmplitude        0.9;
        rotationFrequency        0.25;
        translationAmplitude    (0.0 3.0 0.0);
        translationFrequency    (0.0 0.25 0.0);
        initialRotationOrigin    (0.0 0.0 0.0);
// 		rotationAmplitude		0.707;
// 		rotationFrequency		0.25;
// 		translationAmplitude	(0.0 5.0 0.0);
// 		translationFrequency	(0.0 0.25 0.0);
// 		initialRotationOrigin	(0.0 0.0 0.0);
	}
);

