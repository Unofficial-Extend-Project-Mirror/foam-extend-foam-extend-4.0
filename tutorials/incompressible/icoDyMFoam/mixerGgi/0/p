FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    impellerWall
    {
        type            zeroGradient;
    }
    baffleWall
    {
        type            zeroGradient;
    }
    insideSlider
    {
        type            ggi;
        value           uniform 0;
    }
    outsideSlider
    {
        type            ggi;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
}

