FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.375;

boundaryField
{
    RUINLET
    {
        type            fixedValue;
        value           uniform 0.375;
    }
    RUOUTLET
    {
        type            zeroGradient;
    }
    RUCYCLIC1
    {
        type            cyclicGgi;
        value           uniform 0.375;
    }
    RUCYCLIC2
    {
        type            cyclicGgi;
        value           uniform 0.375;
    }
    RUBLADE
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }
    RUHUB
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }
    RUSHROUD
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }
}

