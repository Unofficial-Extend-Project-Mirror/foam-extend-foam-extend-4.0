// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  CrossPowerLaw;

nu              nu [0 2 -1 0 0 0 0] 1;

CrossPowerLawCoeffs
{
    nu0             nu0 [0 2 -1 0 0 0 0] 0.01;
    nuInf           nuInf [0 2 -1 0 0 0 0] 10;
    m               m [0 0 1 0 0 0 0] 0.4;
    n               n [0 0 0 0 0 0 0] 3;
}

BirdCarreauCoeffs
{
    nu0             nu0 [0 2 -1 0 0 0 0] 1e-06;
    nuInf           nuInf [0 2 -1 0 0 0 0] 1e-06;
    k               k [0 0 1 0 0 0 0] 0;
    n               n [0 0 0 0 0 0 0] 1;
}

