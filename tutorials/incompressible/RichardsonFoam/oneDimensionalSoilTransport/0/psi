FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      psi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform -1;

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform 0.01;
    }

    bottom
    {
        type            fixedGradient;
        gradient        uniform 0;
    }

    sides
    {
    type            empty;
    }
}


