FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions [ 0 2 -3 0 0 0 0 ];

internalField uniform 0.1;

boundaryField
{
    downstreamMixingPlanePatch
    {
        type mixingPlane;
    }
    upstreamMixingPlanePatch
    {
        type mixingPlane;
    }
    downstreamWall
    {
        type symmetryPlane;
    }
    upstreamWall
    {
        type symmetryPlane;
    }
    downstreamPerio1
    {
        type cyclicGgi;
    }
    downstreamPerio2
    {
        type cyclicGgi;
    }
    upstreamPerio1
    {
        type cyclicGgi;
    }
    upstreamPerio2
    {
        type cyclicGgi;
    }
    inflow
    {
        type fixedValue;
        value uniform 0.1;
    }
    outflow
    {
        type zeroGradient;
    }
}

// ************************************************************************* //

