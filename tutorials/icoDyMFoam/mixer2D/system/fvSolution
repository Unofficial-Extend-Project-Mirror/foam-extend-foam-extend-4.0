        maxIter          1000;
        tolerance        1e-02;
        relTol           0;
    };

    p PCG
    {
        preconditioner
        {
            type         DIC;
        }

        minIter          0;
        maxIter          1000;
        tolerance        1e-06;
        relTol           0.05;
    };

    pFinal PCG
    {
        preconditioner
        {
            type         DIC;
        }

        minIter          0;
        maxIter          1000;
        tolerance        1e-06;
        relTol           0;
    };

    U PBiCG
    {
        preconditioner   
        {
            type         DILU;
        }

        minIter          0;
        maxIter          1000;
        tolerance        1e-05;
        relTol           0;
    };

    cellMotionUx PCG
    {
        preconditioner      
        {
            type         DIC;
        }

        minIter          0;
        maxIter          1000;
        tolerance        1e-08;
        relTol           0;
    };
