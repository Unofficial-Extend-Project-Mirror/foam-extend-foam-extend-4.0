FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      V;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    lowValue
    {
        type            fixedValue;
        value           uniform 0;
    }

    highValue
    {
        type            fixedGradient;
        gradient        uniform 60;
    }

    defaultFaces
    {
        type            empty;
    }
}

