FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          sampleDict;
}
 
 // * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
 
 
interpolationScheme cellPoint;
 
writeFormat     raw;
 
sampleSets      
(
    uniform 
    {
        name            cut;
        axis            distance;
        start           (0 0 0);
        end             (0.1 0 0);
        nPoints         100;
    }
);
 
fields
(
//    T
//    TPODreconstruct
    TPOD
);


