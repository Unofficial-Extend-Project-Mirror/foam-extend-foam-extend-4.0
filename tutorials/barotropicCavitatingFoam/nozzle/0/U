FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volVectorField;
    object          U;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    in
    {
        type            pressureInletOutletVelocity;
        phi             phi;
        rho             rho;
        value           uniform (0 0 0);
    }
    out
    {
        type            pressureInletOutletVelocity;
        phi             phi;
        rho             rho;
        value           uniform (0 0 0);
    }
    bottom
    {
        type            symmetryPlane;
    }
    topWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    defaultFaces
    {
        type            empty;
    }
}


