FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    rho  diagonal {};
    rhoU diagonal {};
    rhoE diagonal {};

    U smoothSolver
    {
        smoother         GaussSeidel;
        nSweeps          2;
        tolerance        1e-09;
        relTol           0.01;
    };

    h smoothSolver
    {
        smoother         GaussSeidel;
        nSweeps          2;
        tolerance        1e-10
        relTol           0;
    };
}

