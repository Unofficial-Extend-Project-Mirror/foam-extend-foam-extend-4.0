FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 4.86;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 4.86;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 4.86;
        value           uniform 4.86;
    }
    blade
    {
        type            compressible::kqRWallFunction;
        value           uniform 4.86;
    }
    shaft
    {
        type            compressible::kqRWallFunction;
        value           uniform 4.86;
    }
    shroud
    {
        type            compressible::kqRWallFunction;
        value           uniform 4.86;
    }
    frontAndBack
    {
        type            cyclic;
    }
}


