FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type            symmetryPlane;
    }
    right
    {
//         type            analyticalPlateHoleTraction;
        type            solidTraction;
        traction        uniform (0 0 0);
        pressure        uniform 1e7;
    }
    down
    {
        type            symmetryPlane;
    }
    up
    {
//         type            analyticalPlateHoleTraction;
        type            solidTractionFree;
    }
    hole
    {
        type            solidTractionFree;
    }
    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
