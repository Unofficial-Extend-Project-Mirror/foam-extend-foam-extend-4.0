FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      DU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

axis
{
    type          symmetryPlane;
}

loading
{
    type          timeVaryingFixedDisplacement;
    outOfBounds   clamp;
    fileName      "$FOAM_CASE/constant/timeVsLoadingDisp";
    value         uniform (0 0 0);
}

freeSurface
{
    type          solidTractionFree;
}

bottom
{
    type          fixedDisplacement;
    value         uniform (0 0 0);
}

front
{
    type          wedge;
}

back
{
    type          wedge;
}

}

