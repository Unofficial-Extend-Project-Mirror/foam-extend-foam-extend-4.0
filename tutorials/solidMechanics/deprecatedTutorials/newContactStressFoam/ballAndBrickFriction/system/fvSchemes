FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default     Euler;
}

divSchemes
{
    default         none;
    div(sigma)      Gauss linear;
}

gradSchemes
{
    default leastSquares;
}

laplacianSchemes
{
    default Gauss harmonic corrected;
}

