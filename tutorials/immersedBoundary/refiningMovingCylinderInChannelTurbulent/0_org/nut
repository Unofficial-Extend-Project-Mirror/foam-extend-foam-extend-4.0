FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    ibCylinder
    {
        type            immersedBoundaryNutWallFunction;
        patchType       immersedBoundary;
        setDeadValue    yes;
        deadValue       1e-5;
        value           uniform 0;
    }
    top
    {
        type            zeroGradient;
        value           uniform 0;
    }
    in
    {
        type            zeroGradient;
        value           uniform 0;
    }
    out
    {
        type            zeroGradient;
        value           uniform 0;
    }
    bottom
    {
        type            zeroGradient;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


