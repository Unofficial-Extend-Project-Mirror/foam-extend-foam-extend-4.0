FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 350;

boundaryField
{
    walls
    {
        type            fixedValue;
        value           uniform 400;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 350;
    }
    outlet
    {
        type            zeroGradient;
    }
    cycLeft
    {
        type            cyclic;
    }
    cycRight
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}

