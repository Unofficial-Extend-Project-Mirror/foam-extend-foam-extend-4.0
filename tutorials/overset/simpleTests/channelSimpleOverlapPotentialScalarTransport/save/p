FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          p;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions [0 2 -2 0 0 0 0];

internalField uniform 0;

boundaryField
{
    oversetFaces
    {
        type overset;
        value uniform 0;
        coupledFringe yes;
        conservativeCorrection yes;
        setHoleCellValue  yes;
        holeCellValue     0;
    }
    left
    {
        type zeroGradient;
    }
    leftEnd
    {
        type zeroGradient;
    }
    rightStart
    {
        type zeroGradient;
    }
    right
    {
//         type zeroGradient;

        type fixedValue;
        value uniform 0;
    }
    top
    {
        type zeroGradient;
    }
    bottom
    {
        type zeroGradient;
    }
    frontAndBack
    {
        type empty;
    }
}


