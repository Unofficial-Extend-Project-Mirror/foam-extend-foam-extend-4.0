FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          oversetMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolation
{
    interpolationSchemes (injection);

    default injection;
}

holePatches  ();

regions
(
    left
    {
        index 0;

        // Donor regions
        donorRegions ( right );

        fringe
        {
            type faceCells;
            patches ( leftEnd );
        }
    }

    right
    {
        index 1;

        donorRegions ( left );

        fringe
        {
            type faceCells;
            patches ( rightStart );
        }
    }
);


