FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    oversetFaces
    {
        type overset;
        value uniform (0 0 0);
        coupledFringe yes;

        setHoleCellValue  yes;
        holeCellValue     (0 0 0);
    }
    in
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }
    out
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (1 0 0);
        phi             phi;
    }
    top
    {
        type            slip;
    }
    bottom
    {
        type            slip;
    }
    cylinder
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    outerCylinder
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


