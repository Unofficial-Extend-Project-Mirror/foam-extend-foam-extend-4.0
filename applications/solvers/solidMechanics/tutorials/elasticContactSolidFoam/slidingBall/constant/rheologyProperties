FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      rheologyProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     yes;

rheology
{
    type                    multiMaterial;

    laws
    (
        matZero
        {
            type            linearElastic;
            rho             rho [1 -3 0 0 0 0 0] 7854;
            E               E [1 -1 -2 0 0 0 0] 1e+10;
            nu              nu [0 0 0 0 0 0 0] 0.3;
        }

        matOne
        {
            type            linearElastic;
            rho             rho [1 -3 0 0 0 0 0] 7854;
            E               E [1 -1 -2 0 0 0 0] 2e+11;
            nu              nu [0 0 0 0 0 0 0] 0.3;
        }
    );
}

