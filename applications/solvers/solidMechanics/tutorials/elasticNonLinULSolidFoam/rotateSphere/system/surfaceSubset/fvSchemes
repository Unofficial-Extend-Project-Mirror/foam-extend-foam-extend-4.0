FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


gradSchemes
{
	default		none;

 	snGradCorr(DU) leastSquares;
 	snGradCorr(DUan) leastSquares;
 	snGradCorr(alpha) leastSquares;
}

divSchemes
{
	default none;
}

laplacianSchemes
{
	default none;
}
